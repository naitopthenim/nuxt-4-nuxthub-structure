# Cloudflare API Token (สร้างที่ https://dash.cloudflare.com/profile/api-tokens)
CLOUDFLARE_API_TOKEN=

# Cloudflare Account ID (ดูที่ Workers & Pages → Overview → ขวาล่าง)
CLOUDFLARE_ACCOUNT_ID=

# D1 Database ID (ได้หลังจากรัน: npx wrangler d1 create nuxt-hub-db)
CLOUDFLARE_D1_DATABASE_ID=

# KV Namespace ID (ได้หลังจากรัน: npx wrangler kv namespace create KV)
CLOUDFLARE_KV_NAMESPACE_ID=

# Cache KV Namespace ID (ได้หลังจากรัน: npx wrangler kv namespace create CACHE)
CLOUDFLARE_CACHE_NAMESPACE_ID=

# R2 Bucket Name
CLOUDFLARE_R2_BUCKET_NAME=nuxt-hub-blob

# R2 Binding Name (ตั้งชื่ออะไรก็ได้ เช่น R2_BLOB)
CLOUDFLARE_R2_BINDING_NAME=R2_BLOB